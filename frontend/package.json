    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "naive-ui": "^2.37.3",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
