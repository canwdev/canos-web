    "change-case": "^4",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "mousetrap": "^1.6.5",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "vue": "^3.4.21",
